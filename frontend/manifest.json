{
  "manifest_version": 3,
  "name": "Inject Page Popup",
  "version": "1.0.0",
  "description": "Injects a small popup into the current page",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
